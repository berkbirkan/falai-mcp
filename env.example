# fal.ai API Credentials
# Bu değerleri kendi fal.ai API anahtarlarınızla değiştirin
FAL_KEY=sk_live_your_fal_api_key_here

# Alternatif olarak FAL_KEY_ID ve FAL_KEY_SECRET kullanabilirsiniz
# FAL_KEY_ID=your_key_id
# FAL_KEY_SECRET=your_key_secret

# Model Configuration (opsiyonel)
# FALAI_ALLOWED_MODELS=model1,model2,model3
# FALAI_MODEL_KEYWORDS=image,generation,ai

# Server Configuration
FALAI_ENABLE_HTTP=true
FALAI_HTTP_HOST=0.0.0.0
FALAI_HTTP_PORT=18888
FALAI_REQUEST_TIMEOUT=120
